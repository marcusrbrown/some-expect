{
  "name": "some-expect",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.8",
  "description": "Use some expect() function from a supported testing library.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "postbuild": "type-coverage",
    "watch:build": "pnpm run build -- --watch",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "jest --coverage --passWithNoTests",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "expect",
    "chai",
    "jest",
    "testing"
  ],
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "jest": {
    "preset": "amex-jest-preset"
  },
  "devDependencies": {
    "@types/chai": "4.3.14",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "amex-jest-preset": "7.0.0",
    "chai": "5.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "expect": "29.7.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "type-coverage": "2.28.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "chai": "*",
    "expect": ">=23"
  },
  "peerDependenciesMeta": {
    "chai": {
      "optional": true
    },
    "expect": {
      "optional": true
    }
  },
  "typeCoverage": {
    "atLeast": 86.2,
    "cache": true,
    "detail": true,
    "ignoreCatch": true,
    "strict": true,
    "update": true
  }
}
